#ifndef ARRAYPASSENGER_H_
#define ARRAYPASSENGER_H_
#define TAM 51
#define TAM_FLY 10
typedef struct{
	int id;
	char name[TAM];
	char lastName[TAM];
	float price;
	char flycode[TAM_FLY];
	int typePassenger;
	int statusFlight;
	int isEmpty;
}Passenger;


#endif /* ARRAYPASSENGER_H_ */
