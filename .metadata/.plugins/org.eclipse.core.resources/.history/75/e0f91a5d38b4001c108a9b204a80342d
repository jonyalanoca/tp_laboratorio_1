#include <stdio.h>
#include <stdlib.h>

int menu();
int main(void) {
	setbuf(stdout,NULL);
	switch(menu()){
	case 1:
		break;
	case 2:
		break;
	case 3:
		break;
	case 4:
		break;
	}
	return EXIT_SUCCESS;
}
int menu(){
	printf("%c", 200);
	return 4;
}
