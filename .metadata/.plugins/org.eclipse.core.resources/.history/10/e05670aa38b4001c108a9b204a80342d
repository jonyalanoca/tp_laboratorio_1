#include <stdio.h>
#include <stdlib.h>

int menu();
int main(void) {
	setbuf(stdout,NULL);
	switch(menu()){
	case 1:
		break;
	case 2:
		break;
	case 3:
		break;
	case 4:
		break;
	}
	return EXIT_SUCCESS;
}
int menu(){
	printf("			..[MENU AGENCIA DE VIAJES]..\n");
	printf("			============================\n\n");
	printf("[1]	Ingresar Kilometro.\n");
	printf("[2]	Ingresar Precio de Vuelos.\n");
	printf("[3]	Calcular todos los costos.\n");
	printf("[4]	Informar resultados.\n");
	printf("[5]	Carga forzada de datos.\n");
	printf("[6]	Salir.\n");

	return 4;
}
