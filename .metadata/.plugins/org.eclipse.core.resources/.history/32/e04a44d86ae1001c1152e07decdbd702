#include <stdio.h>
#include <stdlib.h>

typedef struct{
	int id;
	char nombre[20];
	char sexo;
	float altura;
	int isEmpty;
}ePersona;

int main(void) {
	setbuf(stdout,NULL);
	ePersona** empleado=(ePersona**) malloc(sizeof(ePersona*)* 10);
	(*empleado + 1)->isEmpty=12;
	printf("aca: %d\n", (*empleado + 1)->isEmpty);
	scanf("%d", &(*empleado + 1)->isEmpty);
	printf("aca: %d\n", (*empleado + 1)->isEmpty);


	ePersona** empleado2=(ePersona**) malloc (sizeof(ePersona*)*5);
	(*empleado2 + 1)->id=12;
	scanf("%d", &(*empleado2 + 1)->id);
	printf("%d", (*empleado2 + 1)->id);

	return EXIT_SUCCESS;
}
