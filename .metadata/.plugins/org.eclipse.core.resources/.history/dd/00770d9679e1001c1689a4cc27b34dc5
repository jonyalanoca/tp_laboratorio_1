#ifndef TIPO_H_
#define TIPO_H_

typedef struct{
	int id;
	char descripcion[20];
}eTipo;

#endif /* TIPO_H_ */
eTipo** hardCodearTipos(int tamT);
int obtenerIndexPorDescripcionTipo(eTipo** vecTipos,int tamT, char* string);
int obtenerIdPorIndexTipo(eTipo** vecTipos,int tamT, int index);
