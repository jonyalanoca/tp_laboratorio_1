#include <stdio.h>
#include <stdlib.h>

int menu();
int main(void) {
	setbuf(stdout,NULL);
	switch(menu()){
	case 1:
		break;
	case 2:
		break;
	case 3:
		break;
	case 4:
		break;
	}
	return EXIT_SUCCESS;
}
int menu(){
	printf("			..[MENU AGENCIA DE VIAJES]..\n");
	printf("[1]	Ingresar Kilometro.");
	printf("[2]	Ingresar Precio de Vuelos.");
	printf("[3]	Calcular todos los costos.");
	printf("[4]	Informar resultados.");
	printf("[5]	Carga forzada de datos.");
	printf("[6]	Salir.");

	return 4;
}
