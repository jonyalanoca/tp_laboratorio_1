#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int main(void) {
	setbuf(stdout, NULL);
	int numero;
	srand(time(NULL));
	numero=rand();
	printf("%d", numero);
	return EXIT_SUCCESS;
}
